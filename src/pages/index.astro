---
import Layout from "../layouts/Layout.astro";
import Blog from "../components/bloghome.astro"
import Hero from "../components/Herohome.astro";
import CTA from "../components/CTA.astro";
import Logos from "../components/CustomerLogos.astro";
import data from "../data/site.json";
import SmartNextMatch from "../components/SmartNextMatch.astro";
import CalendarUpdater from "../components/CalendarUpdater.astro";
import StatisticsTable from "../components/StatisticsTable.tsx";
import Collab from "../components/Collaborazioni.astro"


---

<Layout title={data.siteName}>
<Hero/>
    <Logos />
    <SmartNextMatch/>
    <CTA />
    <Blog/>
    <StatisticsTable client:load showTitle={true} maxTeams={6} />
    <Collab/>
    <CalendarUpdater />
    
  
</Layout>
<style>
#navhome{
    background: transparent;
}
    </style>