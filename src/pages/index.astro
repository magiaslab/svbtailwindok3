---
import Layout from "../layouts/Layout.astro";
import Blog from "../components/bloghome.astro"
import Hero from "../components/Herohome.astro";
import CTA from "../components/CTA.astro";
import Classifica from "../components/classifica.astro"
import Logos from "../components/CustomerLogos.astro";
import data from "../data/site.json";
import SmartNextMatch from "../components/SmartNextMatch.astro";
import CalendarUpdater from "../components/CalendarUpdater.astro";
import DynamicStats from "../components/DynamicStats.astro";
import Collab from "../components/Collaborazioni.astro"
import Classifica2 from"../components/classifica2.astro"


---

<Layout title={data.siteName}>
<Hero/>
    <Logos />
    <SmartNextMatch/>
    <CTA />
    <Blog/>
<Classifica2 />
<Classifica />
<DynamicStats showTitle={true} showCharts={true} maxTeams={6} />
<Collab/>
<CalendarUpdater />
    
  
</Layout>
<style>
#navhome{
    background: transparent;
}
    </style>