# Forza l'installazione di tutte le dipendenze opzionali
optional=false

# Usa il registry npm ufficiale
registry=https://registry.npmjs.org/

# Installa anche le dipendenze peer
legacy-peer-deps=true

# Forza la risoluzione delle dipendenze opzionali
prefer-offline=false

# Forza l'installazione di dipendenze opzionali specifiche
include-optional=true
